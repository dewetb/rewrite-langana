

<div class="container">
  <div class="row">
    <h3>Find one of <%= pluralize(User.count, 'person') %> in your area, that you can trust to get the job done </h3>
    <div class="column">
      <table class="table">
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <%= render user %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
